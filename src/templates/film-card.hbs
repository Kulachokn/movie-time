{{#each this}}
  <li>
    <div>
      <div>
        <img src="https://image.tmdb.org/t/p/w500{{poster_path}}" alt="{{original_title}}" width="274" height="398">
      </div>
      <div>
        <h2>{{title}}</h2>
        <ul>
          {{#each genres}}
            <li>
              <p>{{this}}</p>
            </li>
          {{/each}}
        </ul>
        <p>{{popularity}}</p>
      </div>
    </div>
  </li>
{{/each}}