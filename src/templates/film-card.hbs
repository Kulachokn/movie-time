{{#each this}}
  <li>
    <div>
      <div>
        <img src="https://www.themoviedb.org/t/p/w600_and_h900_bestv2{{poster_path}}" alt="{{original_title}}" width="274" height="398">
      </div>
      <div>
        <h2>{{title}}</h2>
        <p class="card-genre-date">
          {{genre_ids}} | {{release_date}}
        </p>
        <p>{{popularity}}</p>
      </div>
    </div>
  </li>
{{/each}}